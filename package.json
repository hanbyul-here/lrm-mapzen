  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextzen/lrm-mapzen.git"
  },
  "homepage": "https://github.com/nextzen/lrm-mapzen",
  "bugs": {
    "url": "https://github.com/nextzen/lrm-mapzen/issues"
  },
  "dependencies": {
    "@mapbox/polyline": "^0.2.0",
    "@mapbox/corslite": "0.0.7"
  },
